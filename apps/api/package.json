{
  "name": "@mise/api",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsup src/server.ts --format esm --dts",
    "start": "node dist/server.js",
    "lint": "eslint src/ --ext .ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist .turbo"
  },
  "dependencies": {
    "@mise/shared": "workspace:*",
    "@mise/db": "workspace:*",
    "fastify": "^4.28.0",
    "@fastify/cors": "^9.0.0",
    "@fastify/helmet": "^11.1.0",
    "@fastify/rate-limit": "^9.1.0",
    "@fastify/jwt": "^8.0.0",
    "@fastify/cookie": "^9.3.0",
    "@fastify/multipart": "^8.3.0",
    "pino": "^9.0.0",
    "pino-pretty": "^11.0.0",
    "awilix": "^10.0.0",
    "zod": "^3.23.0",
    "bcrypt": "^5.1.0",
    "ioredis": "^5.4.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.12.0",
    "@types/bcrypt": "^5.0.0",
    "@types/uuid": "^9.0.0",
    "tsx": "^4.7.0",
    "tsup": "^8.0.0",
    "vitest": "^1.6.0",
    "typescript": "^5.4.0"
  }
}
